//Crear una matriz de dos dimensiones con 6 filas y 2 columnas y muestre la suma de cada fila

#include <stdio.h>
int main()
{
    int matriz[6][2]={10,2,5,1,8,4,2,9,5,7,8,3};
    int f;
    int c;
    int s=0;
    
    for(f=0;f<6;f++)
    {
        s=0;
    for(c=0;c<2;c++)
    {
        s=s+matriz[f][c];
    }
    printf("fila: %d, suma: %d\n",f,s);
    }
}
