#include <stdio.h>

void main()
{
     printf("Ingrese el numero que desee:\n");
    int num;
    scanf("%d",&num);
    if(num>= 1 && num <= 7){
        printf("semana 1");
    }
    if(num >= 8 && num<= 15){
        printf("semana 2");
    }
    if(num >= 16 && num<= 23){
        printf("semana 3");
    }
    if(num >= 22 && num<= 31){
        printf("semana 4");
    }
    if(num<1 || num>31)
        {
        printf("no pertenece a ninguna semana");
    }
}
