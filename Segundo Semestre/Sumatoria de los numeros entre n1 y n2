#include <stdio.h>
int main()
{
    int n;
    int n1;
    int n2;
    int suma=0;
    printf("ingrese el valor 1:");
    scanf("%d",&n1);
    printf("ingrese el valor 2:");
    scanf("%d",&n2);
    
    if(n1<n2)
    {
        for(n=n1;n<=n2;n++)
        {
            printf("\n%d",n);
            suma = suma + n;
        }
        printf("\nsumatoria:%d",suma);
    }
    
    else
    {
        printf("valores incorrectos, debe ser n1 < n2");
    }
}
