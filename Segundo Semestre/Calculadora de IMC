void main()
{
   float peso;
   float altura;
   float imc;  
   
   printf("ingrese su peso expresado en kilogramos:\n");
   scanf("%f",&peso);
  
   printf("ingrese su altura expresada en metros:\n");
   scanf("%f",&altura);
   
   imc = peso / (altura * altura);
   printf("imc calculado:%f",imc);
}
