#include <stdio.h>
#include <math.h>

int main()
{
    int a;
    int b;
    int resultado;
    printf("Ingrese el valor de a: ");
    scanf("%d",&a);
    printf("Ingrese el valor de la potencia:");
    scanf("%d",&b);
    resultado=pow(a,b);
    printf("El resultado es: %d",resultado);
    return 0;
}


