#include <stdio.h>

void main()
{
    int n;
    int i;
    int cont=0;
    printf("ingrese un numero entero: ");
    scanf("%d", &n);
    for(i=1; i<=n;i++)
    {
        if(n % i == 0)
        {
            cont++;
        }
    }
    if(cont <= 2)
    {
        printf("El numero es primo");
    } 
    else
    {
        printf("El numero no es primo");
    }
}
