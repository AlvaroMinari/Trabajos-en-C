#include <stdio.h>

int main()
{
    int n1;
    int n2;
    int n3;
  
    printf("Ingrese el primer numnero:");
    scanf("%d",&n1);
    printf("Ingrese el segundo numero:");
    scanf("%d",&n2);
    printf("Ingrese el tercer numero:");
    scanf("%d",&n3);
    
    if (n1>n2 && n1>n3){
        printf("El numero de mayor tamaño es:%d",n1);
    }
    if(n2>n1 && n2>n3){
        printf("El numero de mayor tamaño es:%d",n2);
    }
    if(n3>n1 && n3>n2){
        printf("El numero de mayor tamaño es:%d",n3);
    }
}
